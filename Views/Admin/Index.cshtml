
@{
}
@model IQueryable<CrashNormal>
<table class="table table-sm table-bordered table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Route</th>
            <th>Milepoint</th>
            <th>Road Name</th>
            <th>City</th>
            <th>County</th>
            <th>Severity</th>
            <th>Crash Date</th>
            <th>Crash Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (CrashNormal c in Model)
        {
        <tr>
            <td>@c.crash_id</td>
            <td>@c.route</td>
            <td>@c.milepoint</td>
            <td>@c.main_road_name</td>
            <td>@c.city</td>
            <td>@c.county_name</td>
            <td>@c.crash_severity_id</td>
            <td>@c.crash_date</td>
            <td>@c.crash_time</td>
            <td><a asp-controller="Admin" asp-action="Edit" class="btn btn-sm btn-warning btn-block" asp-route-id="@c.crash_id">Edit</a></td>
            <td><a asp-controller="Admin" asp-action="Delete" class="btn btn-sm btn-danger btn-block" asp-route-id="@c.crash_id">Delete</a></td>
        </tr>
        }

    </tbody>

</table>
<div page-blah=""></div>
